{
	"compilerOptions": {
		"target": "ES5",
		"module": "commonjs",
		"lib": [
			"esnext",
			"DOM"
		],
		"outDir": "./dist",
		"removeComments": false,
		"downlevelIteration": true,
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"strictBindCallApply": true,
		"strictPropertyInitialization": true,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"esModuleInterop": true,
		"watch": false,
		"experimentalDecorators": true,
		"declaration": true,
		"resolveJsonModule": true,
		"baseUrl": "./src/",
		"paths": {
			"@utils": [
				"utils/index.ts"
			],
			"@data": [
				"data.json"
			],
			"@encoder": [
				"encoder/index.ts"
			],
			"@encoder/*": [
				"encoder/*"
			],
			"@decoder": [
				"decoder/index.ts"
			],
			"@decoder/*": [
				"decoder/*"
			],
			"@frames": [
				"frames/index.ts"
			]
		}
	},
	"files": [
		"src/index.ts"
	]
}